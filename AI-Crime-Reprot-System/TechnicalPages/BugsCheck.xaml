<local:BasePageT x:Class="AIFaceDrawing.TechnicalPages.BugsCheck"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:AIFaceDrawing"
      mc:Ignorable="d" 
      Height="470" Width="800"
      Title="BugsCheck">

    <Grid>
        
        <ListBox Name="listBugs" MouseLeftButtonDown="Bug_Click">
            <ListBox.ItemTemplate>
                <DataTemplate>
                    <StackPanel Orientation="Horizontal">
                        <TextBlock Width="800" Height="50" Text="{Binding FullPath}" TextWrapping="WrapWithOverflow"></TextBlock>
                    </StackPanel>
                </DataTemplate>
            </ListBox.ItemTemplate>
        </ListBox>
    </Grid>
</local:BasePageT>
